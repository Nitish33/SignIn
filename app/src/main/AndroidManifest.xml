<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nitishprasad.signin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".topPage"
            android:label="@string/title_activity_top_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".addTeacher"
            android:label="@string/title_activity_add_teacher"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".addStudent"
            android:label="@string/title_activity_add_student"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>